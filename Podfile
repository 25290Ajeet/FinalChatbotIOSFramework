  pod 'KissXML', '~> 5.3.1', :modular_headers => true
  pod 'libidn', '~> 1.35', :modular_headers => true
  pod 'XMPPFramework', '~> 4.0.0', :modular_headers => true
  pod 'MBProgressHUD', '~> 1.1.0', :modular_headers => true

end
